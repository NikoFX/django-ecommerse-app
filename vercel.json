{
    "builds": [
        {
           "src": "denteca/wsgi.py",
           "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "denteca/wsgi.py"
        }
    ]
}
